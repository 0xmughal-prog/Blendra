# ============================================
# GBPb Protocol Deployment Configuration
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# ============================================
# Network Configuration
# ============================================

# Arbitrum Mainnet RPC (get from Alchemy/Infura)
ARBITRUM_RPC=https://arb1.arbitrum.io/rpc

# Etherscan API key for contract verification
ARBISCAN_API_KEY=YOUR_ARBISCAN_API_KEY_HERE

# ============================================
# Deployer Configuration
# ============================================

# Private key of deployer account (KEEP SECURE!)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Treasury address (receives fees)
TREASURY_ADDRESS=0xYOUR_TREASURY_ADDRESS_HERE

# Reserve recipient address
RESERVE_RECIPIENT=0xYOUR_RESERVE_RECIPIENT_HERE

# ============================================
# Deployed Contract Addresses (fill after deployment)
# ============================================

GBPB_ADDRESS=
SGBPB_ADDRESS=
MINTER_ADDRESS=
STRATEGY_ADDRESS=
PERP_MANAGER_ADDRESS=
